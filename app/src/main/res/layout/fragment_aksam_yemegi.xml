<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.AksamYemegiFragment">

    <LinearLayout
        android:id="@+id/LinearLayoutThree"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LinearLayoutOne"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imagebalik"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/balik" />

        <ImageView
            android:id="@+id/imagekarniyarik"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/karniyarik" />

        <ImageView
            android:id="@+id/imagekurufasulye"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/kurufasulye" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutTwo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imagemeyveAksam"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/meyve" />

        <ImageView
            android:id="@+id/imageet"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/et" />

        <ImageView
            android:id="@+id/imagepilav"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/pilav" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutOne"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="21dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/LinearLayoutThree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageetliekmek"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:tag="10"
            app:srcCompat="@drawable/etliekmek" />

        <ImageView
            android:id="@+id/imagesalataAksam"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:tag="20"
            app:srcCompat="@drawable/salata" />

        <ImageView
            android:id="@+id/imagesebzeyemegi"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:tag="30"
            app:srcCompat="@drawable/sebzeyemegi" />

    </LinearLayout>

    <TextView
        android:id="@+id/sonucText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="88dp"
        android:hint="Yediklerinizin Toplam Kalorisi"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="88dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/LinearLayoutThree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/LinearLayoutOne">

        <TextView
            android:id="@+id/etliekmekKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri : "
            android:textAlignment="center" />

        <TextView
            android:id="@+id/etliekmek"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:text="30"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textStyle="bold"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/salataKalori"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri : "
            android:textAlignment="center" />

        <TextView
            android:id="@+id/salata"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:text="52"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/sebzeyemegiKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri : " />

        <TextView
            android:id="@+id/sebzeyemegi"
            android:layout_width="51dp"
            android:layout_height="wrap_content"
            android:text="31"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="MissingConstraints" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="99dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LinearLayoutThree">

        <TextView
            android:id="@+id/balikKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kalori Değeri : "
            android:textAlignment="center" />

        <TextView
            android:id="@+id/balik"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10"
            android:layout_weight="1"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/karniyarikKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kalori Değeri : " />

        <TextView
            android:id="@+id/karniyarik"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="20" />

        <TextView
            android:id="@+id/kurufasulyeKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kalori Değeri : " />

        <TextView
            android:id="@+id/kurufasulye"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="5" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="117dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/sonucText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:id="@+id/meyveKalori"
            android:layout_width="102dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri :"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/meyve"
            android:layout_width="37dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="10" />

        <TextView
            android:id="@+id/etKalori"
            android:layout_width="102dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri :"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/et"
            android:layout_width="31dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="100" />

        <TextView
            android:id="@+id/pilavKalori"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri :"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/pilav"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="10" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="163dp"
        android:layout_height="28dp"
        android:text="kalan kolori için tıklayınız"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sonucText" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="65dp"
        android:layout_height="26dp"
        android:text="Sıfırlayınız"
        app:layout_constraintBottom_toTopOf="@+id/sonucText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

</androidx.constraintlayout.widget.ConstraintLayout>