<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.OgleYemegiFragment">

    <LinearLayout
        android:id="@+id/LinearLayoutThree"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LinearLayoutOne"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageKatmer"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/tavukdoner" />

        <ImageView
            android:id="@+id/imagePatates"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/hamburger" />

        <ImageView
            android:id="@+id/imagePeynir"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/corba" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutTwo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageRecel"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/guvec" />

        <ImageView
            android:id="@+id/imageZeytin"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/pizza" />

        <ImageView
            android:id="@+id/imageBal"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/kuruyemis" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutOne"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="21dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/LinearLayoutThree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageYumurta"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:tag="10"
            app:srcCompat="@drawable/meyve" />

        <ImageView
            android:id="@+id/imageCay"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:tag="20"
            app:srcCompat="@drawable/salata" />

        <ImageView
            android:id="@+id/imageEkmek"
            android:layout_width="54dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:tag="30"
            app:srcCompat="@drawable/iskender" />

    </LinearLayout>

    <TextView
        android:id="@+id/sonucText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="88dp"
        android:hint="Yediklerinizin Toplam Kalorisi"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="88dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/LinearLayoutThree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/LinearLayoutOne">

        <TextView
            android:id="@+id/yumurtaKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri : "
            android:textAlignment="center" />

        <TextView
            android:id="@+id/meyve"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:text="442"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textStyle="bold"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/cayKalori"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri : "
            android:textAlignment="center" />

        <TextView
            android:id="@+id/salata"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:text="30"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/ekmekKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri : " />

        <TextView
            android:id="@+id/iskender"
            android:layout_width="51dp"
            android:layout_height="wrap_content"
            android:text="31"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="MissingConstraints" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="99dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LinearLayoutThree">

        <TextView
            android:id="@+id/katmerKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kalori Değeri : "
            android:textAlignment="center" />

        <TextView
            android:id="@+id/tavukdoner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10"
            android:layout_weight="1"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/patatesKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kalori Değeri : " />

        <TextView
            android:id="@+id/hamburger"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="20" />

        <TextView
            android:id="@+id/peynirKalori"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kalori Değeri : " />

        <TextView
            android:id="@+id/corba"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="5" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="117dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/sonucText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:id="@+id/recelKalori"
            android:layout_width="102dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri :"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/guvec"
            android:layout_width="37dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="10" />

        <TextView
            android:id="@+id/zeytinKalori"
            android:layout_width="102dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri :"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/pizza"
            android:layout_width="31dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="100" />

        <TextView
            android:id="@+id/balKalori"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            android:text="Kalori Değeri :"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/kuruyemis"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="10" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="163dp"
        android:layout_height="28dp"
        android:text="kalan kolori için tıklayınız"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sonucText" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="65dp"
        android:layout_height="26dp"
        android:text="Sıfırlayınız"
        app:layout_constraintBottom_toTopOf="@+id/sonucText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

</androidx.constraintlayout.widget.ConstraintLayout>